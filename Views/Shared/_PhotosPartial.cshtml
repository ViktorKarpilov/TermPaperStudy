@model TermPaper.Models.PhotoPageModel
    <link href="~/css/photo.css" rel="stylesheet">
    
    <!-- Just number of photos, do not touch-->
    <div id="PhotosCounter" style="display:none">@Model.counter</div>

    @for (int i = 0; i < Model.counter; i++)
    {
        
        <div id=@("PhotoContainer"+i) class="PhotoContainer" ><img class="photoBlock" src=@Model.Photos[Model.Photos.Keys.ToArray()[i]] title=@Model.Photos.Keys.ToArray()[i] onload="PhotoLoaded()"></div>
    }
    

    
    
    
